{
  "name": "nopa",
  "version": "0.0.0",
  "description": "Hello Expo!",
  "author": null,
  "private": true,
  "main": "main.js",
  "scripts": {
    "lint": "eslint --fix *.js",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-react-native",
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".+/(resources)/.+",
      ".+/(autoMockOff.js)"
    ],
    "globals": {
      "__DEV__": true
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec|shallow))\\.jsx?$"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "dependencies": {
    "expo": "16.0.0",
    "jest-react-native": "^18.0.0",
    "lodash.debounce": "^4.0.8",
    "react": "16.0.0-alpha.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-16.0.0.tar.gz",
    "react-navigation": "^1.0.0-beta.9",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "chai": "^3.5.0",
    "enzyme": "^2.8.2",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.9.2",
    "eslint-plugin-jsx-a11y": "1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "jest-expo": "^0.4.0",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4"
  }
}
